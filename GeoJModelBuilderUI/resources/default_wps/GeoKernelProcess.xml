<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>GeoKernelProcess</ows:Identifier>
	<ows:Title>Kernel</ows:Title>
	<ows:Abstract>Optionally generates a vector density map on vector
		network with a 1D kernel</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>FirstInputData</ows:Identifier>
			<ows:Title>Vector With Training Points</ows:Title>
			<ows:Abstract>Vector map with training points</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.2.1</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>SecondInputData</ows:Identifier>
			<ows:Title>Vector Network Data</ows:Title>
			<ows:Abstract>Vector network map</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.2.1</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>Stddeviation</ows:Identifier>
			<ows:Title>Stddeviation In Map Units</ows:Title>
			<ows:Abstract>Stddeviation In Map Units</ows:Abstract>
			<LiteralData>
				<ows:DataType ows:reference="xs:double" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>Dsize</ows:Identifier>
			<ows:Title>Discretization Error In Map Units</ows:Title>
			<ows:Abstract>Default:0.0</ows:Abstract>
			<LiteralData>
				<ows:DataType ows:reference="xs:double" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>Length</ows:Identifier>
			<ows:Title>Maximum Length Of Segment On Network</ows:Title>
			<ows:Abstract>Default:100.0</ows:Abstract>
			<LiteralData>
				<ows:DataType ows:reference="xs:double" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>Distance</ows:Identifier>
			<ows:Title>Maximum Distance From Point To Network</ows:Title>
			<ows:Abstract>Default:100.0</ows:Abstract>
			<LiteralData>
				<ows:DataType ows:reference="xs:double" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>Multiply</ows:Identifier>
			<ows:Title>Multiply The Density Result By This Number</ows:Title>
			<ows:Abstract>Default:1.0</ows:Abstract>
			<LiteralData>
				<ows:DataType ows:reference="xs:double" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>A Raster Density Map</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
					<Format>
						<MimeType>application/shp</MimeType>
					</Format>
					<Format>
						<MimeType>application/zip</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.2.1</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>