<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>GeoExtractProcess</ows:Identifier>
	<ows:Title>Extract</ows:Title>
	<ows:Abstract>Selects vector objects from an existing vector map and
		creates a new map containing only the selected objects</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>InputData</ows:Identifier>
			<ows:Title>Vector data</ows:Title>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.2.1</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>List</ows:Identifier>
			<ows:Title>Category values</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>New</ows:Identifier>
			<ows:Title>Enter -1 to keep original categories or the desired NEW
				category value If new >= 0, table is not copied
			</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:int" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="0">
			<ows:Identifier>Typeex</ows:Identifier>
			<ows:Title>Types to be extracted</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="0">
			<ows:Identifier>Type</ows:Identifier>
			<ows:Title>Output File type</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>output</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
					<Format>
						<MimeType>application/shp</MimeType>
					</Format>
					<Format>
						<MimeType>application/zip</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.2.1</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>