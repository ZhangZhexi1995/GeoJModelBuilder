<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>CGAlgorithmsService-length</ows:Identifier>
	<ows:Title>length</ows:Title>
	<ows:Abstract>Computes the length.</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>points</ows:Identifier>
			<ows:Title>the points specifying the line string</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>length</ows:Title>
			<ows:Abstract>the length of the line string</ows:Abstract>
			<LiteralOutput>
				<ows:DataType ows:reference="xs:double" />
			</LiteralOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>