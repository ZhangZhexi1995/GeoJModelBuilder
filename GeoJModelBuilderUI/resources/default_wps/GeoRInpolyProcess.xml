<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>GeoRInpolyProcess</ows:Identifier>
	<ows:Title>Geoinpoly</ows:Title>
	<ows:Abstract>Creates raster maps from ASCII polygon/line/point data
		files</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>InputData</ows:Identifier>
			<ows:Title>input Polyfile</ows:Title>
			<ows:Abstract>the poly file for process</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>text/plain</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>text/plain</MimeType>
					</Format>
					<Format>
						<MimeType>application/rData</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>output in.poly map</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>