<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>GeoDelaunayProcess</ows:Identifier>
	<ows:Title>delaunay triangulation</ows:Title>
	<ows:Abstract>Create a Delaunay triangulation from an input vector map
		containing points or centroids</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>InputData</ows:Identifier>
			<ows:Title>Vector data to build delaunay triangulation</ows:Title>
			<ows:Abstract>the vector map to build delaunay triangulation
			</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.2.1</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>result</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/x-zipped-shp</MimeType>
					</Format>
					<Format>
						<MimeType>application/shp</MimeType>
					</Format>
					<Format>
						<MimeType>application/zip</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/2.1.2.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.0.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.0</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.1.1</MimeType>
					</Format>
					<Format>
						<MimeType>text/xml; subtype=gml/3.2.1</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>