<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>GeoRMfilterProcess</ows:Identifier>
	<ows:Title>Geomfilter</ows:Title>
	<ows:Abstract>Raster file matrix filter</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>InputData</ows:Identifier>
			<ows:Title>Raster Map</ows:Title>
			<ows:Abstract>Input Map</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>FilterData</ows:Identifier>
			<ows:Title>Filter file</ows:Title>
			<ows:Abstract>the matrix filter</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>text/plain</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>text/plain</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>repeat</ows:Identifier>
			<ows:Title>the time of repeat</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:double" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>Result of Mfilter map</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>