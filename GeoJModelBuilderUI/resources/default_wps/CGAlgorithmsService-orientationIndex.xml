<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>CGAlgorithmsService-orientationIndex</ows:Identifier>
	<ows:Title>orientationIndex</ows:Title>
	<ows:Abstract>returns index of the direction of the point relative to a
		vector specified by point1-point2</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>point1</ows:Identifier>
			<ows:Title>the origin point of the vector</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>point2</ows:Identifier>
			<ows:Title>the final point of the vector</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>point</ows:Identifier>
			<ows:Title>the point to compute the direction to</ows:Title>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>orientationIndex</ows:Title>
			<ows:Abstract>1 if point is counter-clockwise (left) from
				point1-point2</ows:Abstract>
			<LiteralOutput>
				<ows:DataType ows:reference="xs:int" />
			</LiteralOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>