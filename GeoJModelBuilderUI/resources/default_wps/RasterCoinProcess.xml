<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>RasterCoinProcess</ows:Identifier>
	<ows:Title>RasterCoin</ows:Title>
	<ows:Abstract>Tabulates the mutual occurrence (coincidence) of
		categories for two raster map layers</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>FirstInputData</ows:Identifier>
			<ows:Title>Raster data</ows:Title>
			<ows:Abstract>Raster data</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>SecondInputData</ows:Identifier>
			<ows:Title>Raster data</ows:Title>
			<ows:Abstract>Raster data</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>Units</ows:Identifier>
			<ows:Title>Unit of measure</ows:Title>
			<ows:Abstract>c(ells), p(ercent), x(percent of category [column]),
				y(percent of category [row]), a(cres), h(ectares), k(square
				kilometers), m(square miles) Options: c,p,x,y,a,h,k,m
			</ows:Abstract>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>coin results</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>