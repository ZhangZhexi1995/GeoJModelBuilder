<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>GeoRCostProcess</ows:Identifier>
	<ows:Title>GeoCost</ows:Title>
	<ows:Abstract>Outputs a raster map layer showing the cumulative cost of
		moving between different geographic locations on an input raster map
		layer whose cell category values represent cost</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>FirstInputData</ows:Identifier>
			<ows:Title>Rast cost Map</ows:Title>
			<ows:Abstract>Rast cost Map</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>SecondInputData</ows:Identifier>
			<ows:Title>start rast map</ows:Title>
			<ows:Abstract>start rast map</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>cumulative cost map</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>