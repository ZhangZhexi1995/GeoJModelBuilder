<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment statusSupported="true" storeSupported="true"
	wps:processVersion="1.1.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ows:Identifier>RasterTransectProcess</ows:Identifier>
	<ows:Title>RasterTransect</ows:Title>
	<ows:Abstract>Outputs raster map layer values lying along user defined
		transect line(s)</ows:Abstract>
	<DataInputs>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>InputData</ows:Identifier>
			<ows:Title>Raster data</ows:Title>
			<ows:Abstract>Raster data</ows:Abstract>
			<ComplexData>
				<Default>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>application/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/geotiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-tiff</MimeType>
					</Format>
					<Format>
						<MimeType>image/x-zipped-geotiff</MimeType>
					</Format>
				</Supported>
			</ComplexData>
		</Input>
		<Input maxOccurs="1" minOccurs="1">
			<ows:Identifier>Line</ows:Identifier>
			<ows:Title>east,north,azimuth,distance[,east,north,azimuth,distance,...]
			</ows:Title>
			<ows:Abstract>east,north,azimuth,distance[,east,north,azimuth,distance,...]r
			</ows:Abstract>
			<LiteralData>
				<ows:DataType ows:reference="xs:string" />
				<ows:AnyValue />
			</LiteralData>
		</Input>
	</DataInputs>
	<ProcessOutputs>
		<Output>
			<ows:Identifier>OutputData</ows:Identifier>
			<ows:Title>describe result</ows:Title>
			<ComplexOutput>
				<Default>
					<Format>
						<MimeType>text/plain</MimeType>
					</Format>
				</Default>
				<Supported>
					<Format>
						<MimeType>text/plain</MimeType>
					</Format>
				</Supported>
			</ComplexOutput>
		</Output>
	</ProcessOutputs>
</xml-fragment>